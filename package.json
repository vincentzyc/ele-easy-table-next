{
  "name": "ele-easy-table-next",
  "version": "1.0.4",
  "private": false,
  "author": "vincentzyc",
  "description": "基于 Vue3、Element Plus 开发的一款表格组件，对Element Plus的el-table进行二次封装",
  "keywords": [
    "component",
    "vue3",
    "element plus",
    "el-table"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/vincentzyc/ele-easy-table-next"
  },
  "types": "./src/ele-easy-table.d.ts",
  "main": "./dist/ele-easy-table-next.umd.js",
  "module": "./dist/ele-easy-table-next.es.js",
  "exports": {
    ".": {
      "import": "./dist/ele-easy-table-next.es.js",
      "require": "./dist/ele-easy-table-next.umd.js"
    }
  },
  "files": [
    "dist/*.css",
    "dist/*.map",
    "dist/*.js",
    "src/*"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview"
  },
  "dependencies": {
    "pinia": "^2.0.9",
    "vue-router": "4.0.12"
  },
  "devDependencies": {
    "@types/node": "^17.0.2",
    "@vitejs/plugin-vue": "^1.10.2",
    "typescript": "^4.4.4",
    "unplugin-auto-import": "^0.5.3",
    "unplugin-vue-components": "^0.17.6",
    "vite": "^2.7.0",
    "vue-tsc": "^0.28.10"
  },
  "peerDependencies": {
    "vue": "^3.2.0",
    "element-plus": "^1.2.0-beta.1",
    "vuedraggable": "^4.1.0"
  }
}