{
  "name": "ele-easy-table-next",
  "version": "1.0.7",
  "private": false,
  "author": "vincentzyc",
  "description": "基于 Vue3、Element Plus 开发的一款表格组件，对Element Plus的el-table进行二次封装",
  "keywords": [
    "component",
    "vue3",
    "element plus",
    "el-table"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/vincentzyc/ele-easy-table-next"
  },
  "types": "./src/ele-easy-table.d.ts",
  "main": "./dist/ele-easy-table-next.umd.js",
  "module": "./dist/ele-easy-table-next.es.js",
  "exports": {
    ".": {
      "import": "./dist/ele-easy-table-next.es.js",
      "require": "./dist/ele-easy-table-next.umd.js"
    }
  },
  "files": [
    "dist/*.css",
    "dist/*.map",
    "dist/*.js",
    "src/*"
  ],
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "build-lib": "vue-cli-service build --target lib --name ele-easy-table-next --dest dist ./src/index.ts"
  },
  "dependencies": {
    "core-js": "^3.29.1",
    "@element-plus/icons-vue": "^2.1.0",
    "element-plus": "^2.3.1",
    "vue": "^3.2.47",
    "vuedraggable": "^4.1.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "@vue/cli-plugin-typescript": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "@vue/eslint-config-typescript": "^9.1.0",
    "eslint": "^7.32.0",
    "eslint-plugin-vue": "^8.0.3",
    "typescript": "~4.5.5"
  }
}