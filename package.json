{
  "name": "ele-easy-table-next",
  "version": "1.0.6",
  "private": false,
  "author": "vincentzyc",
  "description": "基于 Vue3、Element Plus 开发的一款表格组件，对Element Plus的el-table进行二次封装",
  "keywords": [
    "component",
    "vue3",
    "element plus",
    "el-table"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/vincentzyc/ele-easy-table-next"
  },
  "types": "./src/ele-easy-table.d.ts",
  "main": "./dist/ele-easy-table-next.umd.js",
  "module": "./dist/ele-easy-table-next.es.js",
  "exports": {
    ".": {
      "import": "./dist/ele-easy-table-next.es.js",
      "require": "./dist/ele-easy-table-next.umd.js"
    }
  },
  "files": [
    "dist/*.css",
    "dist/*.map",
    "dist/*.js",
    "src/*"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.1.0",
    "element-plus": "^2.3.1",
    "vue": "^3.2.47",
    "vuedraggable": "^4.1.0"
  },
  "devDependencies": {
    "@types/node": "^18.15.9",
    "@vitejs/plugin-vue": "^4.1.0",
    "typescript": "^4.9.3",
    "vite": "^4.2.0",
    "vue-tsc": "^1.2.0"
  }
}
